{
  "manifest_version": 2,

  "name": "Trick or Treat! ^_^",
  "description": "Go trick or treating on your computer!",
  "version": "1.9",
  "icons": {"16":"images/pumpkin128.png","48":"images/pumpkin128.png","128":"images/pumpkin128.png"},
  "browser_action": {
    "default_icon": "images/pumpkin128.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "cookies",
    "contextMenus"
  ]
}
